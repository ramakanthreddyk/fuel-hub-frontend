{
  "name": "fuelsync-backend",
  "version": "1.0.0",
  "description": "FuelSync Hub Backend API",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/app.js",
    "dev": "ts-node src/app.ts",
    "migrate:up": "node scripts/migrate.js up",
    "migrate:down": "node scripts/migrate.js down",
    "migrate:status": "node scripts/migrate.js status",
    "setup-db": "node scripts/setup-unified-db.js",
    "setup-unified-db": "node scripts/setup-unified-db.js",
    "db:migrate": "node scripts/migrate.js up",
    "db:rollback": "node scripts/migrate.js down",
    "db:status": "node scripts/migrate.js status",
    "db:seed": "node scripts/seed-data.js",
    "db:unified-schema": "node scripts/apply-unified-schema.js",
    "db:verify-schema": "node scripts/verify-schema.js",
    "db:fix-constraints": "node scripts/fix-constraints.js",
    "db:seed-data": "node scripts/seed-data.js",
    "add-yearly-price": "node scripts/add-yearly-price.js",
    "check-db": "node scripts/check-db-connection.js",
    "setup-azure-db": "node scripts/setup-azure-db.js",
    "azure-migrate-cash": "node scripts/apply-cash-reports-azure.js",
    "azure-migrate-settings": "node scripts/apply-tenant-settings-kv-azure.js",
    "test": "ts-node scripts/start-db-and-test.ts",
    "test:unit": "jest -c jest.config.ts",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^6.10.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/pg": "^8.10.1",
    "@types/swagger-ui-express": "^4.1.8",
    "axios": "^1.6.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.11.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^20.2.5",
    "jest": "^30.0.4",
    "prisma": "^6.10.1",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.0"
  }
}
